RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond % {REQUEST_FILENAME}\.html -f
RewriteRule ^(.*)$ $1.html [NC,L]


RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond % {REQUEST_FILENAME}\index.html -f
RewriteRule ^(.*)$ $1.html [NC,L]





RewriteEngine On
RewriteCond %{REQUEST_URI} [a-z]
RewriteRule (.*) ${uc:$1} [R=301,L]


